{
  "name": "adblock-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "cjs/**",
          "esm/**",
          "dist/**"
        ]
      },
      "lint": {}
    }
  },
  "resolutions": {
    "immer": "^8.0.1"
  }
}
